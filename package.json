    "redux-form": "7.0.4",
    "redux-thunk": "2.2.0",
    "webpack": "3.5.6",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
